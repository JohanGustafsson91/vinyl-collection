{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16.18.1",
    "npm": "^8.19.2"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && npm run test && next build",
